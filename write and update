#include<stdio.h>
int main()
{
    int a[]={10,20,30,40,50};
    int n=sizeof(a)/sizeof(a[0]);
    int index=34;
    int newvalue=10;
    if (index>=0 && index<n)
    {
        a[index]=newvalue;
        for(int i=0;i<n;i++)
        {
            printf("%d",a[i]);
        }
    }
    else
    {
        printf("Happy sankranthi");
    }
}
